        }
      }
    },
    "TimestampedMaybe": {
      "type": "object",
      "javaType": "com.datafascia.emerge.ucsf.TimestampedMaybe",
      "description": "enum of Yes, No, Contraindicated, Not Documented",
      "properties": {
        "value": {
          "type": "string",
