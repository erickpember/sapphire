    "CareProvider": {
      "type": "object",
      "javaType": "com.datafascia.emerge.ucsf.CareProvider",
      "description": "Care team member",
      "properties": {
        "name": {
          "type": "string",
          "description": "Full name in the format First Middle_1 Middle_2 ... Last, where the Middle names are omitted if they do not exist. The name issued on the first submission for a given care team member must be used in all subsequent submissions"
        },
        "addedToCareTeam": {
          "type": "string",
          "format": "date-time",
          "description": "An explanation about the purpose of this instance described by this schema."
        },
        "updateTime": {
          "type": "string",
          "format": "date-time",
          "description": "When this value was updated"
        }
      }
    },
    "TimestampedBoolean": {
      "type": "object",
      "javaType": "com.datafascia.emerge.ucsf.TimestampedBoolean",
      "properties": {
        "value": {
          "type": "boolean"
        },
        "updateTime": {
          "type": "string",
          "format": "date-time",
          "description": "When the value was updated"
        }
      }
    },
    "TimestampedInteger": {
      "type": "object",
